{
  "name": "graphql_server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "babel src -d dist --source-maps",
    "serve": "cross-env NODE_ENV=production node dist/index.js",
    "start": "cross-env NODE_ENV=development babel-node src/index.js",
    "dev": "cross-env DEBUG=server:debug NODE_ENV=development nodemon src/index.js --exec babel-node",
    "test": " cross-env DEBUG=server:debug NODE_ENV=test mocha --require babel-core/register --reporter spec --exit   src/tests/ --exec babel-node",
    "test:watch": "cross-env DEBUG=server:debug NODE_ENV=development mocha  --require babel-core/register --watch --reporter  spec   tests/ --exec babel-node"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run test",
      "pre-push": "npm run test",
      "...": "..."
    }
  },
  "author": "",
  "license": "ISC",
  "babel": {
    "presets": [
      "node8"
    ]
  },
  "engines": {
    "node": ">=8"
  },
  "eslintConfig": {
    "plugins": [
      "node"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:node/recommended"
    ],
    "rules": {
      "node/no-unsupported-features/es-syntax": 0,
      "node/no-unsupported-features/es-builtins": 0
    },
    "env": {
      "node": true,
      "es6": true
    },
    "parserOptions": {
      "sourceType": "module",
      "ecmaVersion": 2018
    }
  },
  "dependencies": {
    "express": "^4.18.2",
    "express-graphql": "^0.9.0",
    "graphql": "^14.5.8",
    "mongoose": "^5.8.9",
    "uuid": "^3.4.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-node8": "^1.2.0",
    "chai": "^4.2.0",
    "config": "^3.2.5",
    "cross-env": "^7.0.0",
    "husky": "^4.2.1",
    "mocha": "^7.0.0",
    "nodemon": "^2.0.2",
    "prettier": "1.19.1",
    "supertest": "^4.0.2"
  }
}
